<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="8.0dip" />
            <gradient android:startColor="#ffa99d90" android:endColor="#ffa99d90" android:angle="270.0" android:centerY="0.75" android:centerColor="#ffd7d0c3" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="8.0dip" />
                <gradient android:startColor="#ffdb6315" android:endColor="#ffdb6315" android:angle="270.0" android:centerY="0.75" android:centerColor="#fff18800" />
            </shape>
        </clip>
    </item>
</layer-list>
